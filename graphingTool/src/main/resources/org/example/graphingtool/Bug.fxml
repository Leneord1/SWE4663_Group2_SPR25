<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="org.example.graphingtool.BugController" spacing="10" alignment="CENTER">
    <Label text="Bug Tracking" style="-fx-font-size: 18px; -fx-font-weight: bold;"/>

    <Label text="Unpatched Bugs"/>
    <ListView fx:id="unpatchedBugsList"/>

    <Label text="Patched Bugs"/>
    <ListView fx:id="patchedBugsList"/>

    <Label text="Patch Notes"/>
    <TextArea fx:id="patchNotes" prefRowCount="4" editable="false"/>
</VBox>
